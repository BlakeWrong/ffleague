{
  "functions": {
    "api/*.py": {
      "runtime": "python3.9"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ],
  "env": {
    "LEAGUE_ID": "@league_id",
    "ESPN_S2": "@espn_s2",
    "SWID": "@swid"
  },
  "build": {
    "env": {
      "LEAGUE_ID": "@league_id",
      "ESPN_S2": "@espn_s2",
      "SWID": "@swid"
    }
  }
}